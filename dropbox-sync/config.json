{
  "name": "Dropbox Sync",
  "version": "dev",
  "slug": "dropbox_sync",
  "description": "Upload your Hass.io backups to Dropbox",
  "url": "https://github.com/TeemuStenhammar/hassio-dropbox-sync",
  "startup": "before",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "stdin": true,
  "hassio_api": true,
  "hassio_role": "manager",
  "boot": "auto",
  "map": ["backup", "share:rw"],
  "options": {
    "oauth_access_token": "<YOUR_ACCESS_TOKEN>",
    "output": ""
  },
  "schema": {
    "oauth_access_token": "str",
    "output": "str",
    "keep_last": "int(0,)?",
    "filetypes": "str?",
    "rm_uploaded": "bool?"
  },
  "image": "stenhamm/dropbox-sync"
}
